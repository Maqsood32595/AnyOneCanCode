{
  "name": "anyone-can-code",
  "displayName": "Anyone Can Code",
  "description": "A VS Code extension that helps beginners learn to code with interactive features",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:anyone-can-code.helloWorld",
    "onCommand:anyone-can-code.codingTip",
    "onCommand:anyone-can-code.createTests",
    "onCommand:anyone-can-code.showProjectPrompt",
    "onCommand:anyone-can-code.showTestPrompt",
    "onCommand:anyone-can-code.addProjectProtectedPattern",
    "onCommand:anyone-can-code.addTestProtectedPattern",
    "onCommand:anyone-can-code.checkProtectedCode",
    "onCommand:anyone-can-code.viewProtectedPatterns",
    "onCommand:anyone-can-code.showCheckpointPanel",
    "onCommand:anyone-can-code.createCheckpoint",
    "onCommand:anyone-can-code.resetToCheckpoint",
    "onCommand:anyone-can-code.aiChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "anyone-can-code.helloWorld",
        "title": "Anyone Can Code: Hello World"
      },
      {
        "command": "anyone-can-code.codingTip",
        "title": "Anyone Can Code: Get Coding Tip"
      },
      {
        "command": "anyone-can-code.createTests",
        "title": "Anyone Can Code: Create Tests Structure"
      },
      {
        "command": "anyone-can-code.showProjectPrompt",
        "title": "Anyone Can Code: Show Project Review Prompt"
      },
      {
        "command": "anyone-can-code.showTestPrompt",
        "title": "Anyone Can Code: Show Test Review Prompt"
      },
      {
        "command": "anyone-can-code.addProjectProtectedPattern",
        "title": "Anyone Can Code: Add Project Protected Pattern"
      },
      {
        "command": "anyone-can-code.addTestProtectedPattern",
        "title": "Anyone Can Code: Add Test Protected Pattern"
      },
      {
        "command": "anyone-can-code.checkProtectedCode",
        "title": "Anyone Can Code: Check Protected Code"
      },
      {
        "command": "anyone-can-code.viewProtectedPatterns",
        "title": "Anyone Can Code: View Protected Patterns"
      },
      {
        "command": "anyone-can-code.showCheckpointPanel",
        "title": "Anyone Can Code: Show Checkpoint Panel"
      },
      {
        "command": "anyone-can-code.createCheckpoint",
        "title": "Anyone Can Code: Create Checkpoint"
      },
      {
        "command": "anyone-can-code.resetToCheckpoint",
        "title": "Anyone Can Code: Reset to Checkpoint",
        "keybindings": [
          {
            "key": "ctrl+alt+r",
            "mac": "cmd+option+r",
            "when": "editorTextFocus"
          }
        ]
      },
      {
        "command": "anyone-can-code.aiChat",
        "title": "Anyone Can Code: Open AI Chat"
      }
    ],
    "configuration": {
      "title": "Anyone Can Code",
      "properties": {
        "anyoneCanCode.openRouterApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenRouter API key for AI chat functionality"
        },
        "anyoneCanCode.preferredModel": {
          "type": "string",
          "default": "openai/gpt-3.5-turbo",
          "description": "Preferred AI model for chat (e.g., openai/gpt-3.5-turbo, openai/gpt-4)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && node esbuild.mjs",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.mjs --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.mjs --production",
    "check-types": "npx tsc --noEmit",
    "lint": "biome lint --no-errors-on-unmatched --files-ignore-unknown=true --diagnostic-level=error",
    "format": "biome format --changed --no-errors-on-unmatched --files-ignore-unknown=true --diagnostic-level=error",
    "format:fix": "biome check --changed --no-errors-on-unmatched --files-ignore-unknown=true --write",
    "clean:build": "rimraf dist out/",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "typescript": "^5.4.5",
    "@biomejs/biome": "^2.1.4",
    "esbuild": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@aws-sdk/client-bedrock-runtime": "^3.840.0",
    "@google-cloud/vertexai": "^1.9.3",
    "@google/genai": "^1.11.0",
    "@grpc/grpc-js": "^1.9.15",
    "@modelcontextprotocol/sdk": "^1.11.1",
    "@opentelemetry/api": "^1.4.1",
    "@sentry/browser": "^9.12.0",
    "@types/uuid": "^10.0.0",
    "@vscode/codicons": "^0.0.36",
    "axios": "^1.8.2",
    "clone-deep": "^4.0.1",
    "diff": "^5.2.0",
    "execa": "^9.5.2",
    "openai": "^4.83.0",
    "simple-git": "^3.27.0",
    "turndown": "^7.2.0",
    "ulid": "^2.4.0",
    "uuid": "^11.1.0",
    "vscode-uri": "^3.1.0",
    "zod": "^3.24.2"
  }
}